<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>tourism</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>CorsFilter</filter-name>
    <filter-class>org.apache.catalina.filters.CorsFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>CorsFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>HotelBookingController</display-name>
    <servlet-name>HotelBookingController</servlet-name>
    <servlet-class>BookingController.HotelBookingController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HotelBookingController</servlet-name>
    <url-pattern>/bookHotel</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>PackageBookingController</display-name>
    <servlet-name>PackageBookingController</servlet-name>
    <servlet-class>BookingController.PackageBookingController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PackageBookingController</servlet-name>
    <url-pattern>/packageBook</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>TravelBookingController</display-name>
    <servlet-name>TravelBookingController</servlet-name>
    <servlet-class>BookingController.TravelBookingController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TravelBookingController</servlet-name>
    <url-pattern>/bookTravel</url-pattern>
  </servlet-mapping>
</web-app>